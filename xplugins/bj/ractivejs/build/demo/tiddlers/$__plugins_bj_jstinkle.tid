{
    "tiddlers": {
        "$:/plugins/bj/jstinkler/edittemplate": {
            "title": "$:/plugins/bj/jstinkler/edittemplate",
            "type": "text/vnd.tiddlywiki",
            "text": "<div style=\"float:top;vertical-align: top;\">\n<$link><$view field=title/></$link>\n<hr>\n<style>\n.edtithistemplate{\n\t-webkit-box-sizing: border-box;\n\t-moz-box-sizing: border-box;\n\tbox-sizing: border-box;\n\tfloat:top;\n\twidth: 100%;\n\tmax-height:300px;\n\tresize: vertical;\n\tvertical-align: top;\nborder: none;\n}\n</style>\n<$edit-text class=edtithistemplate tiddler=<<currentTiddler>>/>\n</div>\n"
        },
        "$:/plugins/bj/jstinkler/simpletmpt": {
            "title": "$:/plugins/bj/jstinkler/simpletmpt",
            "type": "text/vnd.tiddlywiki",
            "text": "<$link><$view field=title/></$link>\n<hr>\n<$transclude tiddler=<<currentTiddler>>/>\n"
        },
        "$:/plugins/bj/jstinkler/jstinkler": {
            "tags": "$:/tags/Macro",
            "title": "$:/plugins/bj/jstinkler/jstinkler",
            "type": "text/vnd.tiddlywiki",
            "text": "\\define jstinkle(json project)\n<table>\n<$taglist targeTtag=$project$ htmltag=td>\n<$link><<currentTiddler>></$link>\n</$taglist>\n</table>\n<div style=\"float:left;width:50%;vertical-align: top;\"><$mosaic prefix=$json$ wfixed=true index=1 rows=3 cols=1 >{{||$:/plugins/bj/jstinkler/edittemplate}}</$mosaic></div><div style=\"float:left;width:50%;vertical-align: top;\"><$mosaic prefix=$json$ index=2 rows=3 cols=1 >{{||$:/plugins/bj/jstinkler/edittemplate}}</$mosaic></div>\n\n<$mosaic prefix=$json$ wfixed=true index=0 rows=1 cols=9 >\n<$link>\n<<currentTiddler>>\n</$link>\n</$mosaic>\n\\end\n"
        }
    }
}